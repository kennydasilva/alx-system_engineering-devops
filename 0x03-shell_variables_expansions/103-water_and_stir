#!/bin/bash
python3 -c "import os,functools as f;W=os.getenv('WATER','');S=os.getenv('STIR','');A='water';B='stir';C='bestchol';conv=lambda s,a:f.reduce(lambda x,ch:x*len(a)+a.index(ch),[ch for ch in s if ch in a],0);n=conv(W,A)+conv(S,B);L=len(C);out='';exec('while n>0:\n n,r=divmod(n,L)\n out=C[r]+out');print(out or C[0])"
